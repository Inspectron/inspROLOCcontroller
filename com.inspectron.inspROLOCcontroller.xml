<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.inspectron.inspROLOCcontroller">

    <signal name="rolocSignalStrength">
        <arg name="rssi" type="i" direction="out"/>
    </signal>
    <signal name="rolocDepthMeasurement">
        <arg name="depth" type="s" direction="out"/>
    </signal>
    <signal name="rolocStatus">
        <arg name="status" type="i" direction="out"/>
    </signal>
    <signal name="rolocVolume">
        <arg name="volume" type="i" direction="out"/>
    </signal>

    <method name="getSignalStrength">
       <arg name="lvl" type="i" direction="in"/>
    </method>
    <method name="getDepthMeasurement">
       <arg name="lvl" type="i" direction="in"/>
    </method>
    <method name="getStatus">
       <arg name="lvl" type="i" direction="in"/>
    </method>
    <method name="setVolume">
       <arg name="lvl" type="i" direction="in"/>
    </method>

  </interface>
</node>
